def fold ( f, acc, li ) {
    if empty(li) then {
        acc
    } else {
        let a = (f (acc, head(li)));
        let b = (tail(li));
        fold(f, a, b)
    }
};
def fold2 ( f, acc, li ) {
    if empty(li) then {
        acc
    } else {
        halt
    }
};
